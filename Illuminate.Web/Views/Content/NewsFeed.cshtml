@{
    ViewBag.Title = "News feed";
    ViewBag.Module = "content";
}
<script src="../Scripts/angular.js"></script>
<script src="../Scripts/application/newsfeed.controller.js"></script>

<style type="text/css">
     .sidebar-nav-fixed {
        padding: 9px 0;
        position:fixed;
        left:20px;
        top:70px;
        width:215px;
     }
      .row-fluid > .span-fixed-sidebar {
        margin-left: 270px;
     }
 </style>
    <div class="container-fluid">
    <div class="row-fluid row">
       <div class="span10 span-fixed-sidebar">
         <div class="panel panel-primary">
            <div class="panel-heading">
              <h3 class="panel-title">News feed!</h3>
            </div>
            <div class="panel-body">
                <div data-ng-app="content" data-ng-controller="NewsFeedCtrl">
                 <div data-ng-repeat="news in newsFeed">
                  <pre>{{news.Author}} uploaded a new webcast {{news.Title}}
                      Details: {{news.Description}}
                  </pre>
                     <span class="glyphicon glyphicon-thumbs-up"></span>
                     <span class="glyphicon glyphicon-thumbs-down"></span>
                 </div>
                    <button type="button" class="btn btn-default btn-small">
                        <span class="glyphicon glyphicon-thumbs-up"></span>
                    </button>
                      <button type="button" class="btn btn-default btn-small">
                        <span class="glyphicon glyphicon-thumbs-down"></span>
                    </button>

                </div>
            </div>
          </div>
       </div>
     </div>
    </div>